        },
        "ftp": {
            "banner": "220 FTP Server Ready",
            "prompt": "Name: "
        },
        "telnet": {
            "banner": "Welcome to Telnet Server",
            "prompt": "login: "
        },
        "http": {
            "banner": "HTTP/1.1 200 OK",
            "server_header": "Apache/2.4.41 (Win64)"
        }
    }
}
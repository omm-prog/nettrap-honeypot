{
    "honeypot": {
        "ports": [21, 22, 23, 80, 443, 8080, 2222],
        "bind_address": "0.0.0.0",
        "log_level": "INFO",
        "max_connections": 10,
        "banner_delay": 2
    },
    "services": {
        "ssh": {
            "banner": "SSH-2.0-OpenSSH_8.2p1 Ubuntu-4ubuntu0.3",
